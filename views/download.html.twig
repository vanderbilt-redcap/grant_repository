{% extends "base.html.twig" %}

{% block page_content %}
    <h2>Files Uploaded for Grant {{ grant }}</h2>
    {% for index,file in files['files'] %}
        <div><span class="textlink" onclick="downloadFile({{ file|json_encode }},'{{ record }}','{{ userID }}','{{ csrf_token }}')">{{ file.name }}</span></div>
    {% endfor %}
{% endblock %}

{% block js_block %}
    {{ addJS('js/download.js') }}
{% endblock %}
